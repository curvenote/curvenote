{
  "name": "curvenote",
  "version": "0.3.2",
  "description": "Command line tools for Curvenote",
  "author": "Rowan Cockett <rowan@curvenote.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curvenote/curvenotejs.git"
  },
  "bugs": {
    "url": "https://github.com/curvenote/curvenotejs/issues"
  },
  "bin": {
    "curvenote": "./dist/cli/index.js"
  },
  "homepage": "https://github.com/curvenote/curvenotejs",
  "scripts": {
    "clean": "rimraf dist",
    "copy:version": "echo \"const version = '\"$npm_package_version\"';\nexport default version;\" > src/version.ts",
    "dev:cli": "npm uninstall -g curvenote; npm run build; npm link",
    "prepublishOnly": "npm run build",
    "test": "npm run copy:version; jest",
    "test:watch": "npm run copy:version; jest --watchAll",
    "test:clear": "npm run copy:version; jest --clearCache",
    "lint": "npm run copy:version; eslint \"src/**/*.ts\" -c .eslintrc.json",
    "lint:format": "npm run copy:version; prettier --check \"src/**/*.ts\"",
    "lint:format:fix": "npm run copy:version; prettier --write \"src/**/*.ts\"",
    "build:ts": "tsc -p .",
    "build:assets": "copyfiles -f ./src/styles/* ./dist/styles; copyfiles -f ./src/images/* ./dist/images",
    "declarations": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build": "npm-run-all -l copy:version clean -p build:ts build:assets declarations",
    "build:watch": "npm run copy:version; tsc -w -p ."
  },
  "dependencies": {
    "@curvenote/blocks": "^1.3.0",
    "@curvenote/schema": "^0.11.11",
    "@reduxjs/toolkit": "^1.7.2",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "citation-js-utils": "^0.0.3",
    "commander": "^8.3.0",
    "copyfiles": "^2.4.1",
    "date-fns": "^2.25.0",
    "docx": "^7.3.0",
    "html-entities": "^2.3.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^18.0.0",
    "jsonwebtoken": "^8.5.1",
    "katex": "^0.15.2",
    "lodash.throttle": "^4.1.1",
    "mystjs": "^0.0.3",
    "node-fetch": "^2.6.7",
    "pretty-hrtime": "^1.0.3",
    "prosemirror-docx": "^0.0.11",
    "prosemirror-model": "^1.16.1",
    "prosemirror-state": "^1.3.4",
    "redux": "^4.1.2",
    "which": "^2.0.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@types/copyfiles": "^2.4.1",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/katex": "^0.11.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/markdown-it": "^12.2.3",
    "@types/mocha": "^8.2.3",
    "@types/node-fetch": "^2.5.12",
    "@types/pretty-hrtime": "^1.0.1",
    "@types/prosemirror-model": "^1.13.2",
    "@types/prosemirror-state": "^1.2.7",
    "@types/prosemirror-test-builder": "^1.0.2",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-skip-tests": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-test-builder": "^1.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
