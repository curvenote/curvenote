    "myst-spec": "^0.0.5",
    "myst-templates": "^1.0.19",
    "myst-transforms": "^1.3.18",
    "nanoid": "^3.3.2",
    "node-fetch": "^3.3.1",
    "p-limit": "^3.1.0",
    "prosemirror-model": "^1.18.1",
    "redux": "^4.1.2",
    "simple-validators": "^1.0.5",
    "unist-util-select": "^4.0.1",
    "uuid": "^8.3.2",
    "vfile": "^5.0.0",
