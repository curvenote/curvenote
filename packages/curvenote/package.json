{
  "name": "curvenote",
  "version": "0.8.26",
  "description": "Command line tools for Curvenote",
  "author": "Rowan Cockett <rowan@curvenote.com>",
  "license": "MIT",
  "keywords": [
    "markdown",
    "latex",
    "writing-software",
    "scientific-visualization",
    "pdf-generation",
    "science-research"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curvenote/curvenote.git"
  },
  "bugs": {
    "url": "https://github.com/curvenote/curvenote/issues"
  },
  "bin": "./dist/curvenote.cjs",
  "homepage": "https://github.com/curvenote/curvenote",
  "scripts": {
    "clean": "rimraf dist",
    "copy:version": "echo \"const version = '\"$npm_package_version\"';\nexport default version;\" > src/version.ts",
    "unlink": "npm uninstall -g curvenote;",
    "link": "npm run unlink; npm link;",
    "dev": "npm run link && concurrently \"tsc -w --preserveWatchOutput --outDir dist -p .\" \"esbuild src/cli/index.ts --bundle --outfile=dist/curvenote.cjs --platform=node --external:jsdom --external:fsevents --watch\"",
    "lint": "npm run copy:version; eslint \"src/**/*.ts\" -c .eslintrc.cjs",
    "lint:format": "npm run copy:version; prettier --check \"src/**/*.ts\"",
    "build:cli": "esbuild src/cli/index.ts --bundle --outfile=dist/curvenote.cjs --platform=node --external:jsdom --external:fsevents",
    "test": "npm run copy:version; npm run link; vitest run",
    "test:watch": "npm run copy:version; npm run link; vitest watch",
    "build:esm": "tsc --project ./tsconfig.json --module es2015 --outDir dist --declaration",
    "build": "npm-run-all -l clean copy:version -p build:esm build:cli"
  },
  "dependencies": {
    "@curvenote/blocks": "^1.5.24",
    "@curvenote/schema": "0.12.18",
    "@google-cloud/storage": "^5.18.2",
    "@reduxjs/toolkit": "^1.7.2",
    "@wordpress/wordcount": "3.40.0",
    "bottleneck": "^2.19.5",
    "chalk": "^5.2.0",
    "check-node-version": "^4.2.1",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.10.0",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "html-entities": "^2.3.2",
    "http": "^0.0.1-security",
    "inquirer": "^8.2.2",
    "intersphinx": "*",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "jtex": "^1.0.7",
    "mime-types": "^2.1.35",
    "myst-cli": "^1.1.18",
    "myst-cli-utils": "^2.0.4",
    "myst-common": "^1.1.8",
    "myst-config": "^1.1.8",
    "myst-frontmatter": "^1.1.8",
    "myst-spec": "^0.0.4",
    "myst-templates": "^1.0.9",
    "myst-transforms": "^1.1.2",
    "nanoid": "^3.3.2",
    "node-fetch": "^3.3.1",
    "p-limit": "^3.1.0",
    "prosemirror-model": "^1.18.1",
    "redux": "^4.1.2",
    "simple-validators": "^1.0.1",
    "unist-util-select": "^4.0.1",
    "vfile": "^5.0.0",
    "which": "^3.0.1",
    "ws": "^8.9.0"
  },
  "overrides": {
    "prosemirror-tables": "^1.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@types/adm-zip": "^0.5.0",
    "@types/cli-progress": "^3.9.2",
    "@types/copyfiles": "^2.4.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/inquirer": "^8.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.14",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/katex": "^0.11.1",
    "@types/lodash.throttle": "^4.1.6",
    "@types/markdown-it": "^12.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^8.2.3",
    "@types/pretty-hrtime": "^1.0.1",
    "@types/which": "^2.0.1",
    "@types/wordpress__wordcount": "^2.4.2",
    "@types/ws": "^8.5.3",
    "concurrently": "^7.2.2",
    "esbuild": "^0.15.12",
    "eslint": "^8.21.0",
    "eslint-config-curvenote": "latest",
    "memfs": "^3.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-test-builder": "^1.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
